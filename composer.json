{
  "name": "fond-of/php-airtable-sdk",
  "description": "Client Library for the Airtable API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Egger",
      "email": "christopher.egger@fondof.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "FondOf\\Airtable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOf\\Airtable\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "php-coveralls/php-coveralls": "^2.2",
    "phpmd/phpmd": "^2.8",
    "phpro/grumphp-shim": "^0.18",
    "phpstan/phpstan": "^0.12",
    "roave/security-advisories": "dev-master as 1.0.0",
    "sebastian/phpcpd": "^5.0",
    "sllh/composer-versions-check": "^2.0",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
